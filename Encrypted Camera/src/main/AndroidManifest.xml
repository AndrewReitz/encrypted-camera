<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.andrewreitz.encryptedcamera" >

    <application
        android:name="com.andrewreitz.encryptedcamera.EncryptedCameraApp"
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.andrewreitz.encryptedcamera.activity.SettingsActivity"
            android:label="@string/app_name"
            android:launchMode="singleInstance" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.andrewreitz.encryptedcamera.activity.CameraActivity"
            android:label="@string/title_activity_camera" >
        </activity>
    </application>

</manifest>
